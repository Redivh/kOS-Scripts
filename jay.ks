clearscreen.set ship:control:pilotmainthrottle to 0.set t to 1.set flightmode to 0.lock throttle to t.lock p to MIN(sqrt(.116*altitude),90).if flightmode=0 and altitude<500 {   from {local x is 3.} until x=0 step {set x to x-1.} do{      print "..." + x.      wait 1.      }.   lock steering to heading(90,90-p).   stage.   set flightmode to 1.   }.when maxthrust=0 then{   stage.   preserve.   }.when flightmode=1 and apoapsis>80000 then{   set flightmode to 2.   set t to 0.   preserve.   }.when flightmode=2 and eta:apoapsis<15 then{   set p to 90.   set t to 1.   set flightmode to 3.   preserve.   }.when flightmode=3 and periapsis>75000 then{   set t to 0.   set flightmode to 4.   preserve.   }.   until apoapsis > 80000 AND periapsis > 70000 {print "Flightmode = " + flightmode at(0,10).}